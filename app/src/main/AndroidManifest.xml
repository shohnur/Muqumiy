<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uz.muqimiy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Muqimiy">
        <activity
            android:name="uz.muqimiy.ui.SubLyrics"
            android:parentActivityName="uz.muqimiy.ui.LyricsActivity" />
        <activity
            android:name="uz.muqimiy.ui.SubGallery"
            android:parentActivityName="uz.muqimiy.ui.GalleryActivity" />
        <activity
            android:name="uz.muqimiy.ui.GalleryActivity"
            android:parentActivityName="uz.muqimiy.ui.MainActivity" />
        <activity
            android:name="uz.muqimiy.ui.LyricsActivity"
            android:parentActivityName="uz.muqimiy.ui.MainActivity" />
        <activity
            android:name="uz.muqimiy.ui.AboutActivity"
            android:parentActivityName="uz.muqimiy.ui.MainActivity" />
        <activity android:name="uz.muqimiy.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>